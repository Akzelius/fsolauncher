{
  "name": "fsolauncher",
  "version": "1.6.5",
  "description": "Install, play, and configure FreeSO easily",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . fsolauncher --asar --platform=win32 --arch=all --overwrite --electron-version=5.0.3 --version-string.FileDescription='FreeSO Launcher' --out ../../release",
    "builddarwin": "electron-packager . fsolauncher --asar --platform=darwin --arch=all --overwrite --electron-version=5.0.3 --version-string.FileDescription='FreeSO Launcher' --out ../../release",
    "buildasar": "asar pack . \"../../release/asar/app.asar\" --unpack-dir \"{export,FSOLauncher_UI/FSOLauncher_Images,FSOLauncher_UI/FSOLauncher_Sounds,FSOLauncher_UI/FSOLauncher_Fonts}\""
  },
  "keywords": [
    "freeso",
    "launcher"
  ],
  "author": "FreeSO.org",
  "devDependencies": {
    "electron-reload": "^1.5.0",
    "eslint": "^6.7.1"
  },
  "dependencies": {
    "cloudscraper": "^1.5.1",
    "electron-pug": "^1.5.1",
    "follow-redirects": "^1.10.0",
    "fs-extra": "^3.0.1",
    "ini": "^1.3.4",
    "os-locale": "^2.0.0",
    "rss-parser": "^2.7.0",
    "v8-compile-cache": "^2.1.0",
    "winreg": "^1.2.4",
    "yauzl": "^2.10.0",
    "zlibjs": "^0.3.1"
  }
}
